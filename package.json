{
  "name": "browser-date-formatter",
  "version": "3.0.0",
  "description": "Format dates real fancy-like in the browser. Good for search engines and humans.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:chrome && npm run test:firefox && standard && standard-markdown README.md",
    "test:chrome": "browserify test/index.js | smokestack -b chrome | tap-spec",
    "test:firefox": "browserify test/index.js | smokestack -b firefox | tap-spec"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "repository": "https://github.com/zeke/browser-date-formatter",
  "keywords": [
    "browser",
    "browserify",
    "date",
    "time",
    "strftime",
    "format",
    "relative"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
  "license": "MIT",
  "dependencies": {
    "dateutil": "^0.1.0",
    "domready": "^1.0.8",
    "relative-date": "^1.1.3",
    "strftime": "^0.10.0"
  },
  "devDependencies": {
    "brfs": "^2.0.1",
    "browserify": "^13.0.1",
    "jquery": "^3.3.1",
    "smokestack": "^3.4.1",
    "standard": "^9.0.0",
    "standard-markdown": "^2.3.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.5.1"
  }
}
