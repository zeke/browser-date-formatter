{
  "name": "browser-date-formatter",
  "version": "2.0.2",
  "description": "Format dates real fancy-like in the browser. Good for search engines and humans.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:chrome && npm run test:firefox",
    "test:chrome": "browserify test/index.js | smokestack -b chrome | tap-spec",
    "test:firefox": "browserify test/index.js | smokestack -b firefox | tap-spec"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "repository": "https://github.com/zeke/browser-date-formatter",
  "keywords": [
    "browser",
    "browserify",
    "date",
    "time",
    "strftime",
    "format",
    "relative"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeke/browser-date-formatter/issues"
  },
  "homepage": "https://github.com/zeke/browser-date-formatter#readme",
  "dependencies": {
    "dateutil": "^0.1.0",
    "domready": "^1.0.8",
    "prettydate": "0.0.1",
    "relative-date": "^1.1.3",
    "brfs": "^1.4.3"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "jquery": "^2.2.4",
    "smokestack": "^3.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
}
